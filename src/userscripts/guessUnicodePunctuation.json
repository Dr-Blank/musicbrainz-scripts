{
	"name": "MusicBrainz: Guess Unicode punctuation",
	"author": "kellnerd",
	"description": "Searches and replaces ASCII punctuation symbols for many input fields by their preferred Unicode counterparts. Provides “Guess punctuation” buttons for titles, names, disambiguation comments, annotations and edit notes on all entity edit and creation pages.",
	"match": [
		"*://*.musicbrainz.org/*/*/edit_annotation",
		"*://*.musicbrainz.org/artist/*/edit",
		"*://*.musicbrainz.org/artist/create",
		"*://*.musicbrainz.org/event/*/edit",
		"*://*.musicbrainz.org/event/create",
		"*://*.musicbrainz.org/label/*/edit",
		"*://*.musicbrainz.org/label/create",
		"*://*.musicbrainz.org/place/*/edit",
		"*://*.musicbrainz.org/place/create",
		"*://*.musicbrainz.org/recording/*/edit",
		"*://*.musicbrainz.org/recording/create",
		"*://*.musicbrainz.org/release/*/edit",
		"*://*.musicbrainz.org/release/add",
		"*://*.musicbrainz.org/release-group/*/edit",
		"*://*.musicbrainz.org/release-group/create",
		"*://*.musicbrainz.org/series/*/edit",
		"*://*.musicbrainz.org/series/create",
		"*://*.musicbrainz.org/work/*/edit",
		"*://*.musicbrainz.org/work/create"
	]
}
